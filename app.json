{
  "expo": {
    "name": "E-Voice",
    "slug": "EasyInvoice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.png",
    "splash": {
      "image": "./assets/images/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app allows you to upload a logo to be displayed on your generated invoices. Photo library access is required for this reason."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/Icon.png"
    },
    "privacy": "hidden",
    "description": "",
    "githubUrl": "https://github.com/alberthattingh/easy-invoice-client"
  }
}
